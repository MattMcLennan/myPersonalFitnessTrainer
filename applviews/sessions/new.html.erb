<h1>Log In</h1>
<% if flash[:alert] %>
  <hr>
    <%= flash[:alert] %>
  <hr>
<% end %>
<%= form_tag session_path do %>
  <div>
    <%= label_tag :email %><br/>
    <%= text_field_tag :email %>
  </div>
<!--   <div>
    <%= label_tag :password %><br/>
    <%= password_field_tag :password %>
  </div> -->
  <div>
    <%= submit_tag "Log In" %><br/>
  </div>
<% end %>
